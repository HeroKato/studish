<% content_for :head do %>
<title>講師一覧 | Studish</title>
<meta name="description" content="Studishの講師一覧ページです。相性の合う講師を見つけましょう。">
<meta name="keywords" content="Studish,スタディッシュ,オンライン指導,コーチング,Coaching,自学スキルの向上,ディスカッション,discussion">
<% end %>

<div class="content-wrapper" id="coach-list">
  <div class="content-container">
    <div class="content">
      <% if flash.notice %>
        <p class="notice"><%= flash.notice %></p>
      <% end %>
    </div>
    <div class="content">
      <% if @coaches.present? %>
        <%= will_paginate previous_label: '&#8592; &nbsp;前', next_label: '次 &#8594;' %>
        <%= render "coaches/coaches" %>
        <%= will_paginate previous_label: '&#8592; &nbsp;前', next_label: '次 &#8594;' %>
      <% else %>
        <P>講師が一人もいません</P>
      <% end %>
    </div>
  </div>
</div>