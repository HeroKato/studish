<% content_for :head do %>
<title>講師一覧 | Studish</title>
<meta name="description" content="Studishの講師一覧ページです。相性の合う講師を見つけましょう。">
<meta name="keywords" content="Studish,スタディッシュ,オンライン指導,コーチング,Coaching,自学スキルの向上,ディスカッション,discussion">
<% end %>

<div class="page-outer" id="coach-list">
  <div class="page-container" id="coach-stream">
    <% if flash.notice %>
      <p class="notice"><%= flash.notice %></p>
    <% end %>
    <div class="content coach-stream-container">
      <% if @coaches.present? %>
        <%= render "coaches/coach_card" %>
        <%= paginate @coaches %>
      <% else %>
        <P class="nothing">講師は一人もいません</P>
      <% end %>
    </div>
  </div>
</div>