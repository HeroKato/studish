<% content_for :head do %>
<title>講師プロフィール編集 < Studish</title>
<meta name="description" content="Studishの講師のプロフィール編集ページです。">
<meta name="keywords" content="Studish,スタディッシュ,オンライン指導,コーチング,Coaching,自学スキルの向上,ディスカッション,discussion">
<% end %>

<div class="content-wrapper" id="coach_new">
  <div class="content-container">
    <div class="content-header">
      <h2><%= @coach.name %>さんのプロフィール編集</h2>
    </div>
    <div class="content">
      <%= form_for @coach do |form| %>
        <%= render "form", form: form %>
        <div class="submit-group">
          <%= form.submit "更新する", class: "btn btn-success" %>
        </div>
      <% end %>
    </div>
  </div>
</div>